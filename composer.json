{
    "name": "ekreative/redmine_login",
    "description": "Login to symfony using redmine as user provider",
    "license": "MIT",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Ekreative\\RedmineLoginBundle\\": "src/Ekreative/RedmineLoginBundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2",
        "mcfedr/json-form": "^3.2.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/framework-bundle": "^3.0|^4.0",
        "symfony/form": "^3.0"
    },
    "require-dev": {
        "symfony/monolog-bundle": "^3.0|^2.4",
        "phpunit/phpunit": "^5.3",
        "symfony/phpunit-bridge": "^4.2"
    }
}
